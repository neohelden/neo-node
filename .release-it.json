{
  "non-interactive": true,
  "force": false,
  "increment": "patch",
  "commitMessage": "Release v%s",
  "tagName": "v%s",
  "tagAnnotation": "Release v%s",
  "git": {
    "requireCleanWorkingDir": true
  },
  "scripts": {
    "changelog": "git log --pretty=format:'* %s (%h)' [REV_RANGE]"
  },
  "npm": {
    "publish": true
  },
  "github": {
    "release": true
  }
}
